{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/", "dest": "/src/index.html" }
  ]
}
